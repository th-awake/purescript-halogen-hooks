{
  "name": "halogen-hooks",
  "private": true,
  "version": "0.0.1",
  "description": "Hooks for Halogen",
  "repository": "https://github.com/thomashoneyman/purescript-halogen-hooks",
  "author": "Thomas Honeyman <hello@thomashoneyman.com>",
  "license": "MIT",
  "scripts": {
    "postinstall": "node node_modules/puppeteer/install.js",
    "clean": "rm -rf .spago output node_modules *.lock",
    "test-normal": "npm run build:test-normal && npm run run:test-normal",
    "build:test-normal": "spago -x test/test.dhall bundle-app --main Performance.Test.App --to test/test.js --minify",
    "run:test-normal": "node test-normal.mjs",
    "test-optimized": "npm run build:test-optimized && npm run run:test-optimized",
    "build:test-optimized": "spago -x test/test.optimized.dhall bundle-app --main Performance.Test.App --to test/test.js --minify",
    "run:test-optimized": "node test-optimized.mjs"
  },
  "devDependencies": {
    "filter-console": "^0.1.1",
    "headless-devtools": "^2.0.1",
    "puppeteer": "^13.7.0",
    "tracealyzer": "^0.10.3"
  }
}
